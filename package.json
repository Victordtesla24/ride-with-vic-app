{
  "name": "ride-with-vic",
  "version": "1.0.0",
  "description": "Track your rides, get fare estimates, and create receipts",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "build": "next build",
    "test": "node test/test.js",
    "run-tests": "node scripts/run-tests.js",
    "lint": "eslint .",
    "create": "node scripts/cli-create.js",
    "postinstall": "husky install",
    "check-structure": "node scripts/enforce-structure.js",
    "clean-duplicates": "node -e \"import('./scripts/enforce-structure.js').then(mod => mod.findDuplicateFiles().forEach(d => console.log(d.name, d.locations.join(', '))));\"",
    "report-structure": "node scripts/enforce-structure.js --report-only",
    "schedule-structure-check": "node scripts/schedule-structure-check.js"
  },
  "keywords": [
    "ride",
    "tracker",
    "uber",
    "lyft",
    "tesla",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "jose": "^4.14.4",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "leaflet": "^1.9.4",
    "next": "^13.4.13",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "^13.5.11",
    "husky": "^8.0.3",
    "puppeteer": "^20.9.0"
  },
  "type": "module"
}
