{
  "name": "ride-with-vic",
  "version": "1.0.0",
  "description": "Track your rides, get fare estimates, and create receipts",
  "main": "server/server.cjs",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node server/server.cjs",
    "dev": "NODE_ENV=development node server/server.cjs",
    "dev:next": "node scripts/start-nextjs.js",
    "build": "next build",
    "test": "node test/index.js",
    "test:jest": "jest",
    "test:detect-mocks": "node test/utils/detect-mocks.js",
    "test:detect-duplicates": "node test/utils/detect-duplicates.js",
    "test:check-requirements": "node test/utils/check-requirements.js",
    "test:validate": "npm run test:detect-mocks && npm run test:detect-duplicates && npm run test:check-requirements",
    "test:ci": "npm run test:validate && npm run test:jest",
    "test:legacy": "node test/test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fix-imports": "node scripts/fix-imports.js",
    "create": "node scripts/cli-create.js",
    "postinstall": "husky install",
    "check-structure": "node scripts/enforce-structure.js",
    "clean-duplicates": "node -e \"import('./scripts/enforce-structure.js').then(mod => mod.findDuplicateFiles().forEach(d => console.log(d.name, d.locations.join(', '))));\"",
    "report-structure": "node scripts/enforce-structure.js --report-only",
    "schedule-structure-check": "node scripts/schedule-structure-check.js",
    "tesla-api-test": "node scripts/test-tesla-api.js",
    "tesla-api-test:curl": "bash scripts/test-tesla-api.sh",
    "tesla-token-test": "node scripts/test-tesla-token.js",
    "uber-api-test": "bash scripts/test-uber-api.sh",
    "uber-token-test": "node scripts/test-uber-token.js",
    "export": "next build && next export",
    "vercel-build": "next build",
    "docker-build": "docker build -t ride-with-vic .",
    "docker-run": "docker run -p 3000:3000 -e PORT=3000 ride-with-vic"
  },
  "keywords": [
    "ride",
    "tracker",
    "uber",
    "lyft",
    "tesla",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@mui/x-date-pickers": "^6.20.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.7",
    "jose": "^4.14.4",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "next": "^13.4.13",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.22.10",
    "@eslint/js": "^9.24.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "babel-jest": "^29.7.0",
    "chalk": "^5.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^13.5.11",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-mocks-http": "^1.16.2",
    "puppeteer": "^24.6.0"
  }
}
